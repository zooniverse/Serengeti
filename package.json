{
  "name": "serengeti",
  "version": "0.0.1",
  "dependencies": {
    "es5-shimify": "0.0.1",
    "hem": "0.1.9",
    "t7e": "0.2.3",
    "jqueryify": "1.8.0",
    "json2ify": "0.0.1",
    "nib": "0.8.2",
    "serveup": "~0.0.5",
    "spine": "1.0.7",
    "underscore": "1.4.2",
    "zooniverse": "zooniverse/Zooniverse#98ffd4b",
    "zootorial": "git://github.com/brian-c/zootorial#e94c0e2",
    "moment": "1.7.2",
    "publisssh": "0.1.3"
  },
  "scripts": {
    "start": "hem server --port 6371",
