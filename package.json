{
  "name": "serengeti",
  "version": "0.0.1",
  "dependencies": {
    "es5-shimify": "~0.0.1",
    "hem": "~0.1.8",
    "http": "0.0.0",
    "jqueryify": "~0.0.1",
    "json2ify": "~0.0.1",
    "moment": "~1.7.2",
    "nib": "~0.8.2",
    "publisssh": "~0.1.3",
    "querystring": "^0.2.0",
    "serveup": "~0.0.5",
    "spine": "~1.0.7",
    "t7e": "~0.2.3",
    "underscore": "~1.4.2",
    "zooniverse": "zooniverse/Zooniverse#98ffd4b",
    "zootorial": "~0.2.0"
  },
  "scripts": {
    "start": "hem server --port 6371",
    "deploy-to-staging": "hem build && publisssh ./public zooniverse-demo/serengeti && rm -rf ./public/application.*",
    "deploy-to-production": "hem build && publisssh ./public zooniverse-static/www.snapshotserengeti.org && rm -rf ./public/application.*",
    "deploy-to-preview": "hem build && publisssh ./public zooniverse-static/preview.zooniverse.org/serengeti && rm -rf ./public/application.*"
  }
}
